{
    "name": "@sky-monitor/monitor-sdk-react",
    "version": "1.0.0",
    "main": "build/cjs/index.js",
    "module": "build/esm/index.mjs",
    "types": "build/types/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "types": "./build/types/index.d.ts",
                "default": "./build/esm/index.mjs"
            },
            "require": {
                "types": "./build/types/index.d.ts",
                "default": "./build/cjs/index.js"
            }
        }
    },
    "scripts": {
        "build:watch": "pnpm build:watch:transpile & pnpm build:watch:types",
        "build:watch:transpile": "tsup --watch",
        "build:watch:types": "tsc -p tsconfig.types.json --watch",
        "build": "pnpm build:transpile && pnpm build:types",
        "build:transpile": "tsup",
        "build:types": "tsc -p tsconfig.types.json",
        "clean": "rimraf build",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "react",
        "monitoring",
        "error-tracking",
        "error-boundary"
    ],
    "author": "",
    "license": "MIT",
    "description": "React integration for Sky Monitor SDK",
    "peerDependencies": {
        "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
    },
    "dependencies": {
        "@sky-monitor/monitor-sdk-core": "workspace:*",
        "@sky-monitor/monitor-sdk-browser": "workspace:*"
    },
    "devDependencies": {
        "@types/react": "^18.0.0"
    }
}
